package com.bharath.first;

import java.util.Scanner;

public class B {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the string");
		String s=scan.next();
		int a = 0;
		for(int i=0;i<s.length();i++)
		{
			char c=s.charAt(i);
			if(c>='0' && c<='9')
			{
				 a=c-'0';
				 continue;
			}
			for(int j=1;j<=a;j++)
			{
				System.out.print(c);
			}
		}

	}

}
